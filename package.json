{
  "name": "vue-editable-markdown",
  "version": "0.1.3",
  "description": "Click to Edit Textarea with markdown support",
  "author": "Yılmaz Çakmakçı <cakmakcy@gmail.com> ",
  "homepage": "https://github.com/yilmazcakmakci/vue-editable-markdown",
  "bugs": {
    "url": "https://github.com/yilmazcakmakci/vue-editable-markdown/issues"
  },
  "main": "dist/editable-markdown.ssr.js",
  "browser": "dist/editable-markdown.esm.js",
  "module": "dist/editable-markdown.esm.js",
  "unpkg": "dist/editable-markdown.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "keywords": [
    "markdown",
    "click to edit",
    "editable",
    "textarea"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "jest"
  },
  "dependencies": {
    "dompurify": "^2.0.11",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.0.3",
    "marked": "^1.1.0",
    "vue-textarea-autosize": "^1.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "private": false,
  "engines": {
    "node": ">=10"
  }
}
